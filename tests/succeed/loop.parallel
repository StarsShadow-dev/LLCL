function main(): Int8 {
	printLine("Starting loop.");

	var i: Int32 = 0;
	while (i < 5) {
		printLine("Looping!");
		i = i + 1;
	}

	return 0;
}

function printLine(string: Pointer): Void {
	printf(string);
	putchar(10); // \n
	
	return;
}

function putchar(char: Int32): Int32;
function printf(pointer: Pointer): Int32;