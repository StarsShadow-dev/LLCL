import "modules/libc_bindings"

struct Vector3 {
	var x: Float32;
	var y: Float32;
	var z: Float32;
}

function Vector3_new(x: Float32, y: Float32, z: Float32): Vector3 {
	var newVector: Vector3;
	newVector.x = x;
	newVector.y = y;
	newVector.z = z;
	return newVector;
}

function Vector3_add(v1: Vector3, v2: Vector3): Vector3 {
	return Vector3_new(
		v1.x + v2.x,
		v1.y + v2.y,
		v1.z + v2.z
	);
}

function Vector3_subtract(v1: Vector3, v2: Vector3): Vector3 {
	return Vector3_new(
		v1.x - v2.x,
		v1.y - v2.y,
		v1.z - v2.z
	);
}