import "modules/libc_bindings"

struct Vector3 {
	var x: Float32;
	var y: Float32;
	var z: Float32;
}

function Vector3_add(vector1: Vector3, vector2: Vector3): Vector3 {
	var newVector: Vector3;
	newVector.x = vector1.x + vector2.x;
	newVector.y = vector1.y + vector2.y;
	newVector.z = vector1.z + vector2.z;
	return newVector;
}