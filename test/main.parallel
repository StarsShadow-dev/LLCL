function main(): Int8 {
	putchar(97); // a

	if (true) {
		putchar(98); // b
	}

	if (getTrue()) {
		putchar(99); // c
	}

	if (getFalse()) {
		putchar(100); // d
	}

	putchar(10); // \n

	return 0;
}

function getTrue(): Bool {
	return true;
}

function getFalse(): Bool {
	return false;
}

function putchar(char: Int32): Int32 "
declare i32 @putchar(i32 noundef)
