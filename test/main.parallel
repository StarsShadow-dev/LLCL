function main(): Int8 {
	// This is probably the most convoluted way I have ever put 'abc' to the standard out.
	var a: Int32 = 97;
	var b: Int32 = a + 1;
	var ABdifference: Int32 = b - a;

	putchar(b - ABdifference);
	putchar(a + ABdifference);
	putchar(a + ABdifference + 1);

	putchar(10); // \n

	return 0;
}

function printLine(string: Pointer): Void {
	printf(string);
	putchar(10); // \n

	return;
}

function putchar(char: Int32): Int32 "declare i32 @putchar(i32 noundef)"
function printf(pointer: Pointer): Int32 "declare i32 @printf(ptr noundef, ...)"