function main(): Int8 {
	var i:Int32 = 0;

	while (i == 0) {
		putchar(97);
		i = i + 1;
	}

	return 0;
}

function printLine(string: Pointer): Void {
	printf(string);
	putchar(10); // \n
	
	return;
}

function putchar(char: Int32): Int32 "declare i32 @putchar(i32 noundef)"
function printf(pointer: Pointer): Int32 "declare i32 @printf(ptr noundef, ...)"