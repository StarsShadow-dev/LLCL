function main(): Int8 {
	var i: Int32 = 97;

	putchar(i);
	putchar(i + 1);
	putchar(i + 2);
	putchar(i + 3);
	putchar(i + 4);
	putchar(i + 5);
	putchar(i + 6);

	putchar(10); // \n

	return 0;
}

function printLine(string: Pointer): Void {
	printf(string);
	putchar(10); // \n

	return;
}

function putchar(char: Int32): Int32 "declare i32 @putchar(i32 noundef)"
