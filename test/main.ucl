Color = enum {
	Red
	Green
	Blue
	// Custom(r: Number, g: Number, b: Number)
	// Custom(struct(r: Number, g: Number, b: Number) {})
}

getColorText = fn(color: Color): String {
	match color {
		Red -> {
			"red"
		}
		Green -> {
			"green"
		}
		Blue -> {
			"blue"
		}
		// Custom(rgb) -> {
		// 	"custom(\(rgb.r), \(rgb.g), \(rgb.b))"
		// }
	}
}

@compDebug(getColorText(Color.Green))
