@onCodeGen("fn", fn(name: String) {
	@addCodeGen("top", "function ");
	@addCodeGen("top", name);
	@addCodeGen("top", "() {\n");
	@moveCodeGen("top", "fn_out");
	@addCodeGen("top", "}\n");
});

const log = fn(string: String) {
	@addCodeGen("fn_out", "console.log(\"", string, "\");\n");
};

const exportTest = fn() {
	log("Hello, World!");
	log("Bye, World!");
};

@export("test", exportTest);

@writeToStdout("top");