Color = enum {
	Red
	Green
	Blue
	// Custom(r: Number, g: Number, b: Number)
}

getColorText = fn(color: Color): String {
	match color {
		Red -> {
			"red"
		}
		Green -> {
			"green"
		}
		Blue -> {
			"blue"
		}
		// Custom(r, g, b) -> {
		// 	"custom(\(r), \(g), \(b))"
		// }
	}
}

@compDebug(getColorText(Color.Blue))
