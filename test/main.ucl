Color = enum {
	Red
	Green
	Blue
	Custom = struct (
		r: Number,
		g: Number,
		b: Number
	) {}
}

getCustomColorText = fn(rgb: Color.Custom): String {
	"custom(\(rgb.r), \(rgb.g), \(rgb.b))"
}

getColorText = fn(color: Color): String {
	match color {
		Red -> {
			"red"
		}
		Green -> {
			"green"
		}
		Blue -> {
			"blue"
		}
		Custom(rgb) -> {
			// @compDebug(rgb)
			getCustomColorText(rgb)
		}
	}
}

@compDebug(getColorText(Color.Red))
@compDebug(getColorText(Color.Green))
@compDebug(getColorText(Color.Blue))
@compDebug(getColorText(Color.Custom {
	r = 1
	g = 2
	b = 3
}))