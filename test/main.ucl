@onCodeGen("fn_start", fn() {
	@codeGen("__out", "function name() {\n");
});

@onCodeGen("fn_end", fn() {
	@codeGen("__out", "}");
});

const log = fn(string: String) {
    @codeGen("__out", "console.log(\"log\", string);\n");
};

const exportTest = fn() {
	log("Hello, World!");
	log("Bye, World!");
};

@export("test", exportTest);

@writeTofile("__out", "./out/main.js");