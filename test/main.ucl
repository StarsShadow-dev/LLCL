const fibonacci = fn(n: Number): Number {
	if (n < 1) {
		ret n;
	}
	if (n == 1) {
		ret n;
	}
	ret fibonacci(n - 1) + fibonacci(n - 2);
};
@export("fibonacci", fibonacci);

